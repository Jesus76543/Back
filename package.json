    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "day": "^0.0.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "js": "^0.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "rate-limiter-flexible": "^7.1.1"
  },
  "devDependencies": {

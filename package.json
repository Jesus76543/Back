{
  "name": "proyecto",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node --import tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "setup:security": "node scripts/generate-secrets.js",
    "security:check": "node scripts/security-check.js",
    "security:deps": "node scripts/dependency-check.js",
    "security:validate": "npm run build && node -e \"import('./dist/config/environment.js').then(m => m.validateEnvironment())\"",
    "security:full": "npm run security:check && npm run security:deps && npm run security:validate",
    "prestart": "npm run security:validate",
    "postinstall": "npm run setup:security"
  },
  "keywords": [],
